# .env.dist

###> symfony/framework-bundle ###
# Environment: can be 'dev', 'test', or 'prod'
APP_ENV=dev
# Secret key for your Symfony application; replace with your own unique secret
APP_SECRET=YOUR_APP_SECRET
###< symfony/framework-bundle ###

###> doctrine/doctrine-bundle ###
# Database connection URL
# Replace with your database credentials: 
# DATABASE_URL format example:
# "mysql://username:password@host:port/dbname?serverVersion=version&charset=utf8mb4"
DATABASE_URL="mysql://username:password@host:port/ecom?serverVersion=8.0.32&charset=utf8mb4"
###< doctrine/doctrine-bundle ###

###> lexik/jwt-authentication-bundle ###
# Path to JWT private and public keys; configure as needed
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
# JWT passphrase placeholder
JWT_PASSPHRASE=YOUR_JWT_PASSPHRASE
###< lexik/jwt-authentication-bundle ###